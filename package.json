{
  "name": "ai-assistente",
  "version": "1.0.0",
  "description": "Assistente pessoal com IA usando GROQ",
  "main": "electron/main.js",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:electron",
    "build:frontend": "cd frontend && npm run build",
    "build:electron": "cd frontend && npm run build:electron",
    "start": "electron frontend/public/electron.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": ["ai", "assistant", "electron", "react", "groq"],
  "author": "Breno Ribeiro",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
